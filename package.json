{
  "name": "CUTIE",
  "version": "1.9.0",
  "author": "Practera",
  "homepage": "https://github.com/intersective/cutie",
  "localsegment": "<SEGMENT_KEY>",
  "scripts": {
    "ng": "ng",
    "start": "SEGMENT_KEY=$(jq -r .localsegment package.json); [[ ! -z \"$SEGMENT_KEY\" ]] && echo Local SEGMENT_KEY exist ||  SEGMENT_KEY='<SEGMENT_KEY>'; sed -i '' \"s/<SEGMENT_KEY>/$SEGMENT_KEY/g\" src/index.html; ng serve --host 0.0.0.0 --port 4202 --disable-host-check --ssl true",
    "dev": "npm run start",
    "build": "ng build --base-href './' && npx cap copy",
    "test": "ng test --watch=false",
    "lint": "ng lint app",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "deploy": "ng build -c demo --base-href '/cutie/' && npx angular-cli-ghpages --dir=www",
    "sonar-test": "ng test --no-watch --code-coverage --browsers ChromeHeadless --karmaConfig=karma.conf.sonar.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@apollo/client": "^3.5.9",
    "@ionic/angular": "^7.4.1",
    "@ionic/storage": "^4.0.0",
    "@ng-bootstrap/ng-bootstrap": "^14.2.0",
    "@popperjs/core": "^2.11.8",
    "@swimlane/ngx-charts": "^20.4.1",
    "@swimlane/ngx-datatable": "20.1.0",
    "ansi-colors": "^4.1.1",
    "apollo-angular": "^4.2.1",
    "bootstrap": "5.2.3",
    "core-js": "^3.21.1",
    "filestack-js": "^3.27.0",
    "graphql": "^16.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ngx-quill": "^20.0.0",
    "pusher-js": "^8.3.0",
    "quill": "^1.3.7",
    "quill-delta": "^5.1.0",
